let input=["Madam","malayalam","soundar","mom","kumar","Dad","krishna"];
let out=[];
(function(){
let palindrome=function(data){
for(let i=0;i<data.length;i++){
    let array=data[i].split('').reverse().join("");
    if(array.toLowerCase() ===data[i].toLowerCase()){
    out.push(data[i]);
    }
}
console.log(out);
}
palindrome(input);
})();


